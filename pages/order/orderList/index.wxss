.calendar-box {
  display: flex;
  align-items: center;
  background: #fff;
  height: 80rpx;
  padding: 24rpx;
  box-sizing: border-box;
}
.list-box {
  margin-top: 24rpx;
}
.list-box .item {
  background: #fff;
  margin-bottom: 24rpx;
}
.list-box .item .top {
  box-sizing: border-box;
  padding: 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list-box .item .middle {
  box-sizing: border-box;
  padding: 24rpx;
  display: flex;
  align-items: center;
}
.list-box .item .middle .middle-left {
  flex: 1;
  display: flex;
  align-items: center;
}
.list-box .item .middle .middle-left image {
  margin-right: 24rpx;
  width: 120rpx;
  height: 120rpx;
  background: #ddd;
}
.list-box .item .bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx;
  box-sizing: border-box;
}
.list-box .item .bottom .right {
  text-align: right;
}
