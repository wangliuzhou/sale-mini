.list {
  padding: 12rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.list .item-box {
  width: 50%;
}
.list .item-box .item {
  background: #fff;
  margin: 12rpx;
  text-align: center;
  padding: 50rpx;
  border-radius: 8rpx;
}
.list .item-box .item image {
  width: 100rpx;
  height: 100rpx;
  margin-bottom: 24rpx;
}
