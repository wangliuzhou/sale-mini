.main-title {
  background: #fff;
  font-size: 28rpx;
  font-weight: bolder;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 24rpx;
  border-bottom: 1px solid #eee;
}
.public {
  padding: 24rpx;
  box-sizing: border-box;
  background: #fff;
  .img {
    width: 120rpx;
    height: 120rpx;
  }
  .title {
    display: flex;
  }
}
.input-pre {
  width: 180rpx;
}
.flexa {
  display: flex;
  align-items: center;
  margin-bottom: 0rpx;
}
