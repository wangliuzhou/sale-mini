.image-box {
  text-align: center;
  background: #fff;
  padding: 24rpx 0;
}
.image-box image {
  width: 200rpx;
  height: 180rpx;
}
.que-huo-box {
  background: #fff;
  padding: 24rpx;
  box-sizing: border-box;
  margin: 24rpx 0;
}
.que-huo-box .title {
  font-weight: 600;
}
.que-huo-box .item {
  display: flex;
  align-items: center;
  margin-top: 36rpx;
}
.que-huo-box .item .left {
  width: 120rpx;
  height: 120rpx;
  margin-right: 24rpx;
}
.list-title {
  background: #fff;
  display: flex;
  padding: 24rpx;
  box-sizing: border-box;
}
.list-title .service-num {
  flex: 0.4;
}
.list-title .service-location {
  flex: 0.4;
}
.list-title .service-less-box {
  flex: 0.2;
}
.service-list {
  background: #fff;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.service-list .item {
  margin-bottom: 48rpx;
}
.service-list .item .top {
  display: flex;
  width: 100%;
  align-items: center;
}
.service-list .item .top .service-num {
  flex: 0.4;
  display: flex;
  align-items: center;
}
.service-list .item .top .service-location {
  flex: 0.4;
}
.service-list .item .top .service-less-box {
  flex: 0.2;
}
.service-list .item .top .icon-image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 8rpx;
}
.service-list .item .top .service-less-box {
  display: flex;
  flex-wrap: wrap;
}
.service-list .item .top .service-less-box .less-box {
  background: #ffc118;
  padding: 4rpx 8rpx;
  box-sizing: border-box;
  color: #fff;
  border-radius: 4rpx;
  margin-right: 12rpx;
  margin-bottom: 4rpx;
}
.service-list .item .bottom {
  display: flex;
  flex-wrap: wrap;
}
.service-list .item .bottom .item {
  width: 320rpx;
  display: flex;
  align-items: center;
}
.service-list .item .bottom .item image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 12rpx;
}
.service-list .item .bottom .item .name {
  font-size: 24rpx;
}
.service-list .item .bottom .item .flex {
  display: flex;
}
.service-list .item .bottom .item .flex .number {
  background: #ffc118;
  padding: 4rpx 8rpx;
  box-sizing: border-box;
  color: #fff;
  border-radius: 4rpx;
}
